<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
<l:layout title="Testing radio block in repeatable">
  <l:main-panel>
    <f:form method="post" name="config" action="submitTest">
      <f:repeatable var="foo" items="${it.list}" name="foos">
        <table>
          <f:entry>
            <f:textbox name="txt" value=""/>
          </f:entry>
          <f:radioBlock name="radio" value="one" title="Choice One" checked="${true}">
            <f:entry title="a">
              <f:textbox name="a" value=""/>
            </f:entry>
          </f:radioBlock>
          <f:radioBlock name="radio" value="two" title="Choice Two" checked="${false}">
            <f:entry title="b">
              <f:textbox name="b" value=""/>
            </f:entry>
          </f:radioBlock>
        </table>
      </f:repeatable>
      <f:entry>
        <f:submit value="submit"/>
      </f:entry>
    </f:form>
  </l:main-panel>
</l:layout>
</j:jelly>
