<!--
  Config page
-->
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
<l:layout title="Creating all kinds of fun @nameRef tree">
  <l:main-panel>
    <f:form method="post" name="config" action="submitTest1">
      <f:entry>
        <div name="foo" id="foo"/>
        <div name="bar" id="bar" nameRef="foo" />
        <div nameRef="bar">
          <input type="text" name="zot" value="zot" />
        </div>
      </f:entry>
      <f:entry>
        <f:submit value="submit"/>
      </f:entry>
    </f:form>
  </l:main-panel>
</l:layout>
</j:jelly>
