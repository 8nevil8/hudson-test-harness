<!--
  Config page
-->
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
<l:layout title="Testing the effect of validateButton">
  <l:main-panel>
    <f:form method="post" name="config" action="submitTest1">
      <f:dropdownList name="goalType">
        <f:dropdownListBlock value="buildGoal" title="Build Goal" selected="true">
          <f:entry title="a">
            <f:textbox name="a" value="avalue" />
          </f:entry>
        </f:dropdownListBlock>
        <f:dropdownListBlock value="findbugsGoal" title="FindBugs Goal">
          <f:entry title="b">
            <f:textbox name="b" value="bvalue" />
          </f:entry>
        </f:dropdownListBlock>
      </f:dropdownList>
      <f:entry>
        <f:submit value="submit"/>
      </f:entry>
    </f:form>
  </l:main-panel>
</l:layout>
</j:jelly>
